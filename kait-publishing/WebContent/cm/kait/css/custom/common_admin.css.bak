.topnav{margin-top:-40px; height:40px}
.topnav ul:after{content:'';display:block;clear:both;}
.topnav>ul>li{position:relative;float:left;line-height:40px;text-align:center;height:40px;}
.topnav>ul>li>a{margin:8px 0 16px;display:block;position:relative;width:128px;height:16px;font-size:16px;color:#464646;line-height:16px;border-right:1px solid #a6a6a6}
.topnav>ul {padding-left:215px;min-width:1246px;}
.topnav>ul>li.active>a,.topnav>ul>li>a:hover,.topnav>ul>li>a:focus,.topnav>ul>li>a:active{font-family:'微软雅黑 Regular', '微软雅黑', "Microsoft Yahei", Arial, sans-serif;color:#a3509a;font-weight:bold;}
.topnav ul li:last-child a{border-right:0}
.topnav ul li.active a .arr_btm{margin-left:-7px;position:absolute;left:50%;bottom:-16px;border-bottom:7px solid #464646;border-left:7px solid transparent;border-right:7px solid transparent;}
.topnav ul li a .arr_right.down{position:absolute;top:5px;right:15px;border-top:6px solid #464646;border-left:5px solid transparent;border-right:5px solid transparent;}
.topnav ul li a .arr_right.up{position:absolute;top:5px;right:15px;border-bottom:6px solid #464646;border-left:5px solid transparent;border-right:5px solid transparent;}
.topnav ul li a:hover .arr_right.down,.topnav ul li a:active .arr_right.down,.topnav ul li a:focus .arr_right.down,.topnav ul li.active a .arr_right.down{border-top:6px solid #a3509a;border-left:5px solid transparent;border-right:5px solid transparent;}
.topnav ul li a:hover .arr_right.up,.topnav ul li.active a .arr_right.up{border-bottom:6px solid #a3509a;border-left:5px solid transparent;border-right:5px solid transparent;}

.topnav>ul>li.active .gnb_sub{display:block;}
.gnb_sub{display:none;position:fixed;left:0;right:0;min-width:1246px;}
.gnb_sub ul:after{content:'';display:block;clear:both;}
.gnb_sub ul{position:absolute;left:224px;display:inline-block;}
.gnb_sub.v2 ul{left:353px;}
.gnb_sub.v3 ul{left:480px;}
.gnb_sub.v4 ul{left:610px;}
.gnb_sub.v5 ul{left:740px;}
.gnb_sub.v6 ul{left:868px;}
.gnb_sub.v7 ul{left:1000px;}
.gnb_sub.v8 ul{left:1115px;}
.gnb_sub ul li{float:left;line-height:40px;text-align:center;height:40px;}
.gnb_sub ul li a{margin:4px 0 0 0;padding:0 30px;display:block;position:relative;height:33px;font-size:13px;color:#fff;line-height:33px;border:0;}
.gnb_sub ul li a:hover,.gnb_sub ul li a:focus,.gnb_sub ul li a:active,.gnb_sub ul li.active a{color:#f0ba00;border-bottom: 3px solid #f0ba00;}

.topnav>ul>li.active .menu_all{display:block;}
.dis_none{display:none;}
.menu_all{display:none;position:fixed;left:0;right:0;margin:0 41px 0 41px;padding:10px 0 10px 15px;min-width:1162px;height:auto;border:1px solid #464646;background:#fff;box-shadow: 3px 3px 3px rgba(0,0,0,.1)}
.menu_all:after{content:'';display:block;clear:both;}
.menu_all .menu_all_box{margin-right:1%;float:left;width:15.66666666666667%;min-height:204px;border-right:1px solid #cdcdcd;box-sizing:border-box;}
.menu_all .menu_all_box>ul{margin-right:8%;min-height:204px;height:100%;border-top:1px solid #a3509a;}
.menu_all .menu_all_box:nth-of-type(-n+6)>ul{margin-right:0;border-top:0;}
.menu_all .menu_all_box:nth-child(6),.menu_all .menu_all_box:nth-child(12){border-right:0}
.menu_all .menu_all_box:nth-child(6)>ul,.menu_all .menu_all_box:nth-child(12)>ul{margin-right:0;}
.menu_all .menu_all_box>ul>li{padding:5px 0;font-size:15px;text-align:left;line-height:25px;}
.menu_all .menu_all_box>ul>li:last-child{border:0}
.menu_all .menu_all_box>ul>li>a{display:inline-block;color:#464646;}
.menu_all .menu_all_box>ul>li>a:hover,.menu_all>ul>li>a:focus, .menu_all>ul>li>a:active, .menu_all>ul>li.active>a{color:#630c5a;font-weight:bold}
.menu_all .menu_all_box ul li li{font-size:12px; line-height:21px}
.menu_all .menu_all_box ul li li a{color:#464646;}
.menu_all .menu_all_box ul li li a:hover,.menu_all .menu_all_box ul li li a:focus,.menu_all .menu_all_box ul li li a:active{color:#630c5a;font-weight:bold}
