@CHARSET "UTF-8";

@import url("/ix/css/custom/xg_style.css");

/* Element Reset */
body,table,input,textarea,select,button{color:#817d81;font-size:12px;font-family:'微软雅黑 Regular', '微软雅黑', "Microsoft Yahei", Arial, sans-serif;}
header,footer,section,article,aside,nav,hgroup,details,menu,figure,figcaption{display: block}
button,input[type=submit],input[type=reset],input[type=button]{cursor:pointer;overflow:visible;}
img{border:0}
h1, h2, h3, h4, h5, h6, dl, dd{margin: 0;}
p[tabindex="0"],
div[tabindex="0"]{outline:0;}
img, fieldset{border:0;}
form,figure,fieldset{margin: 0;padding: 0;}
textarea{resize:vertical;}
em{font-style:normal;}
img[src=""],
img[src=" "]{visibility:hidden;}
button{margin:0;padding:0;border:1px solid #d9d9d9;background-color: #f4f4f4; outline: none;}
ol,ul{margin:0;padding:0;list-style:none;}
a,a:hover,a:active,a:focus{text-decoration:none;}
.show{display:block!important;}
.hide{display:none!important;}
.center{text-align:center;}
.right{text-align: right;}
.left{text-align: left;}
.vr{display:inline-block;width:1px;height:14px;margin:0 6px;background:#d4d4d4;overflow:hidden;vertical-align:middle;}
.sr-only{position:absolute!important;height:1px!important;overflow:hidden!important;opacity:0!important;filter:alpha(opacity=0);border:0;margin:-1px;}
.spinner{vertical-align: middle;}

.BR_none{border-right: 0px none !important;}
.BL_none{border-left: 0px none !important;}
.top{vertical-align: top !important;}

/* 말줄임 */
/* Firefox 에서 줄 바꿈이 가능하도록 변경 */
/* .ellipsis,x:-moz-any-link,x:default{white-space:normal;word-wrap:break-word} 
위 행이 IE7에도 적용되므로 IE7에만 줄 바꿈이 불가능하도록 다시 복원 */
*+html .ellipsis{white-space:nowrap} 
.ellipsis {display: block!important;text-overflow:ellipsis;-o-text-overflow:ellipsis;overflow:hidden;white-space:nowrap; word-wrap:normal!important; cursor: default;} /* 말줄임 */
.ellipsis { -moz-binding: url('ellipsis.xml#ellipsis');}

/* 글자색 */
.red{color: #f34465!important;}
.yellow{color: #ffcc33!important;}
.green{color: #00aa50!important;}

