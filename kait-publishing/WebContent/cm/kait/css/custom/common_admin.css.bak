.sift_alert .alert_text{margin-top: 40px; margin-bottom:20px; font-size: 20px;}
.sift_alert > dl{border-bottom: 1px solid #ccc; margin: }
.sift_alert > dl > dt,
.sift_alert > dl > dd{display: inline-block; vertical-align: middle; height: 57px;font-size: 15px;}
.sift_alert > dl > dt{width: 25%}
.sift_alert > dl > dd{width: 70%}

.sift_alert2{width: 400px; height: 470px; margin: -200px 0px 0px -150px; background: #f6f6f6; text-align: center; position: fixed; top: 50%; left: 50%; box-shadow: 3px 3px 5px 1px rgba(0,0,0,0.2); border: 1px solid #dadada;z-index: 1;}
.sift_alert2 .alert_text{margin-top: 40px; margin-bottom:20px; font-size: 20px;}
.sift_alert2 > dl{border-bottom: 1px solid #ccc; margin: 0px 20px 20px; }
.sift_alert2 > dl > dt,
.sift_alert2 > dl > dd{display: inline-block; vertical-align: middle; height: 40px;font-size: 18px; text-align: left;}
.sift_alert2 > dl > dt{width: 35%; font-weight: }
.sift_alert2 > dl > dd{width: 60%}
.sift_alert2 > dl > capcha{display: inline-block; vertical-align: middle; height: 90px;font-size: 18px; text-align: left; width: 100%;}
.sift_alert2 > dl > capchaText{display: inline-block; vertical-align: middle; height: 30px;font-size: 18px; text-align: left; width: 100%;}

.sift_alert2 > dl > dd > input[type="password"],
.sift_alert2 > dl > dd > input[type="text"]{border: 0px none; background: transparent; outline: none; padding: 5px ; min-height: 29px; width: 100%; box-sizing: border-box;}
.sift_alert2 > dl > capchaText > input[type="password"]{border: 0px none; background: transparent; outline: none; padding: 5px ; min-height: 29px; width: 100%; box-sizing: border-box;}



.sift_alert3 {text-align: center; position: fixed; }
